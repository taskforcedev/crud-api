{
  "name": "taskforcedev/crud-api",
  "description": "A laravel package providing an admin interface to create, edit and delete models.",
  "authors": [
    {
      "name": "Taskforcedev",
      "email": "taskforce2eu@gmail.com"
    }
  ],
  "license": "MIT",
  "keywords": ["admin", "views", "laravel", "crud"],
  "require": {
    "php": ">=5.6.4",
    "laravel/framework": ">=5.4",
    "taskforcedev/laravel-support": "1.1.*"
  },
  "require-dev": {
    "phpunit/phpunit": "5.*",
    "mockery/mockery": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Taskforcedev\\CrudApi\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "extra": {
    "laravel": {
      "providers": [
        "Taskforcedev\\CrudApi\\ServiceProvider"
      ]
    }
  }
}
